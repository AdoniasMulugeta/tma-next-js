{
  "name": "nextjs-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:https": "next dev --experimental-https",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:undo": "npx sequelize-cli db:seed:undo",
    "db:migration:create": "npx sequelize-cli migration:generate --name",
    "db:seed:create": "npx sequelize-cli seed:generate --name"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.51.11",
    "@telegram-apps/react-router-integration": "^1.0.0",
    "@telegram-apps/sdk-react": "^1.0.0",
    "@telegram-apps/telegram-ui": "^2.1.5",
    "@tonconnect/ui-react": "^2.0.5",
    "eruda": "^3.0.1",
    "next": "14.2.4",
    "normalize.css": "^8.0.1",
    "pg": "^8.12.0",
    "react": "^18",
    "react-dom": "^18",
    "sequelize": "^6.37.3"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/pg": "^8.11.6",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
